{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "axios": false
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "no-console": ["error", { "allow": ["warn", "error", "log"] }],
      "no-alert": ["error", { "allow": ["warn", "error"] }]
      
    }
}